{
  "name": "api_gateway",
  "version": "0.1.0",
  "description": "Api Gateway for Surprisin social e-commerce platform",
  "author": "Ali Hajiloo",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "docker:up": "docker compose -f docker-compose.yaml up api",
    "docker:up:build": "docker compose -f docker-compose.yaml up api --build",
    "docker:up:dev": "docker compose -f docker-compose.development.yaml up",
    "docker:up:build:dev": "docker compose -f docker-compose.development.yaml up --build",
    "docker:up:test": "docker compose -f docker-compose.test.yaml up",
    "docker:up:build:test": "docker compose -f docker-compose.test.yaml up --build",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@golevelup/nestjs-rabbitmq": "^4.0.0",
    "@grpc/grpc-js": "^1.13.4",
    "@grpc/proto-loader": "^0.7.10",
    "@nestjs/common": "^10.3.0",
    "@nestjs/core": "^10.3.0",
    "@nestjs/microservices": "10.3.0",
    "@nestjs/mongoose": "^11.0.3",
    "@nestjs/platform-express": "^10.3.0",
    "@nestjs/platform-socket.io": "^10.3.3",
    "@nestjs/swagger": "^7.1.17",
    "@nestjs/websockets": "^10.3.3",
    "@types/multer": "^1.4.11",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "dotenv": "^16.3.1",
    "lodash": "^4.17.21",
    "long": "^5.2.3",
    "mongoose": "^8.15.0",
    "nest-winston": "^1.9.4",
    "pg": "^8.7.1",
    "reflect-metadata": "^0.2.1",
    "rimraf": "^5.0.5",
    "rxjs": "^7.8.1",
    "swagger-ui-express": "^5.0.0",
    "traverse": "^0.6.7",
    "uuid": "^11.1.0",
    "webpack": "^5.89.0",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@compodoc/compodoc": "^1.1.23",
    "@nestjs/cli": "^10.2.1",
    "@nestjs/schematics": "^10.0.3",
    "@nestjs/testing": "^10.3.0",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/mongoose": "^5.11.97",
    "@types/node": "^20.10.5",
    "@types/socket.io": "^3.0.2",
    "@types/supertest": "^2.0.16",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "esdoc": "^1.1.0",
    "esdoc-standard-plugin": "^1.0.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "^5.1.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "resolutions": {
    "glob": "10.3.10"
  }
}
